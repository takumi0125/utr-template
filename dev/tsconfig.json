{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": false
  }
}